{
  "name": "borc-components",
  "version": "0.0.1",
  "description": "Beer or Coffee Web components",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-boilerplate/stencil-boilerplate.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build-storybook": "build-storybook",
    "build-site": "npm run build && npm run build-storybook",
    "dev": "stencil build --watch --serve",
    "generate": "stencil generate",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "serve": "serve www -p 5000",
    "start": "run-p dev storybook",
    "storybook": "start-storybook -p 6006 -s dist/",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "devDependencies": {
    "@acce/lit-extended": "^1.0.1",
    "@babel/core": "^7.11.0",
    "@stencil/core": "^1.17.2",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/sass": "^1.4.1",
    "@storybook/addon-essentials": "^6.0.0-rc.26",
    "@storybook/web-components": "^6.0.0-rc.26",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.2.2",
    "lit-html": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "webpack": "^4.44.1",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "author": "Thiago Resende",
  "license": "MIT",
  "dependencies": {}
}
